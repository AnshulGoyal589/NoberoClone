    setInterval(function() {
        currentIndex = (currentIndex + 1) % imageCount;
        const translateValue = -currentIndex * 100 + '%';
        slider.style.transform = 'translateX(' + translateValue + ')';
    }, 2000); // Change image every 2 seconds (2000 milliseconds)
</script>
